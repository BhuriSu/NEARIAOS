{
    "extends": [
      "plugin:cypress/recommended",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react"],
    "rules": { "react/prop-types": "off" }
  }